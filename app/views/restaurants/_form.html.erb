<%= form_for(restaurant) do |f| %>
  <%= f.label :title, "標題" %>
  <%= f.text_field :title %><br>
    
  <%= f.label :address, "地址" %>
  <%= f.text_field :address %><br>
    
  <%= f.label :tel, "電話" %>
  <%= f.text_field :tel %><br>
    
  <%= f.label :email, "信箱" %>
  <%= f.text_field :email %><br>
    
  <%= f.label :description, "簡介" %>
  <%= f.text_area :description %><br>
    
  <%= f.submit %>
    
<% end %>